{
    "log": {
        "disabled": false,
        "level": "debug",
        "timestamp": true
    },
    "dns": {
        "servers": [
            {
                "tag": "local-dns",
                "type": "local"
            },
            {
                "tag": "google-doh",
                "type": "https",
                "server": "8.8.8.8",
                "server_port": 443,
                "path": "/dns-query",
                "tls": {
                    "record_fragment": true
                }
            }
        ],
        "rules": [
            {
                "domain_suffix": [
                            ".ir"
                        ],
                "action": "route",
                "server": "local-dns"
            }
        ],
        "final": "google-doh",
        "strategy": "ipv4_only"
    },
    "inbounds": [
        {
            "type": "mixed",
            "tag": "router-in",
            "listen_port": 1080,
            "sniff": true
        }
    ],
    "outbounds": [
        {
            "type": "direct",
            "tag": "direct"
        }
    ],
    "endpoints": [
        {
            "type": "wireguard",
            "tag": "warp-out",
            "system": false,
            "address": [
                "10.0.0.1/32"
            ],
            "private_key": "YOUR_PRIVATE_KEY",
            "peers": [
                {
                    "address": "warp_auto",
                    "port": 2408,
                    "public_key": "bmXOC+F1FxEMF9dyiK2H5\/1SUtzH0JuVo51h2wPfgyo=",
                    "allowed_ips": [
                        "0.0.0.0/0"
                    ]
                }
            ],
            "mtu": 1280
        }
    ],
    "route": {
        "rule_set": [
            {
                "type": "inline",
                "tag": "iran",
                "rules": [
                    {
                        "domain_suffix": [
                            ".ir"
                        ]
                    }
                ]
            },
            {
                "type": "inline",
                "tag": "dns-servers",
                "rules": [
                    {
                        "domain_regex": [
                            "(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\.)+cloudflare-dns\\.com",
                            "(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\.)+adguard-dns\\.com"
                        ]
                    },
                    {
                        "domain": [
                            "1.1.1.1",
                            "1.1.1.2",
                            "1.1.1.3",
                            "8.8.8.8",
                            "8.8.4.4",
                            "one.one.one.one"
                        ]
                    }
                ]
            },
            {
                "type": "inline",
                "tag": "youtube",
                "rules": [
                    {
                        "domain_regex": [
                            "(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\.)*youtube\\.com",
                            "(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\.)*youtu\\.be",
                            "(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\.)*ytimg\\.com",
                            "(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\.)*ggpht\\.com",
                            "(?:[a-zA-Z0-9](?:[a-zA-Z0-9-]{0,61}[a-zA-Z0-9])?\\.)*googlevideo\\.com"
                        ]
                    },
                    {
                        "domain": [
                            "youtube-ui.l.google.com"
                        ]
                    }
                ]
            },
            {
                "type": "remote",
                "tag": "geosite-ir",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geosite-ir.srs",
                "download_detour": "direct"
            },
            {
                "type": "remote",
                "tag": "geoip-ir",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geoip-ir.srs",
                "download_detour": "direct"
            }
        ],
        "rules": [
            {
                "rule_set": [
                    "iran",
                    "geosite-ir",
                    "geoip-ir"
                ],
                "outbound": "direct"
            },
            {
                "rule_set": [
                    "dns-servers",
                    "youtube"
                ],
                "action": "route-options",
                "outbound": "direct",
                "tls_record_fragment": true
            }
        ],
        "default_domain_resolver": {
            "server": "google-doh"
        },
        "final": "warp-out"
    }
}
